{
  "name": "rentoma",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://rentoma.anozon.me/",
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "dotenv": "8.1.0",
    "firebase": "5.11.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "promise": "8.0.3",
    "react": "16.10.2",
    "react-dev-utils": "6.1.1",
    "react-dom": "16.10.2",
    "react-fontawesome": "1.6.1",
    "react-helmet": "5.2.1",
    "react-images-upload": "1.2.7",
    "react-redux": "5.1.2",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.8",
    "react-scroll-to-component": "1.0.2",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "styled-components": "4.4.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.12",
    "@storybook/addon-links": "3.4.12",
    "@storybook/addons": "3.4.12",
    "@storybook/react": "3.4.12",
    "babel-core": "6.26.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-s2s-action-creater": "0.3.1",
    "babel-plugin-s2s-action-root": "0.4.2",
    "babel-plugin-s2s-action-types": "0.4.11",
    "babel-plugin-s2s-reducer-root": "1.0.1",
    "babel-plugin-s2s-state-root": "0.2.10",
    "babel-plugin-styled-components": "1.10.6",
    "babel-runtime": "6.26.0",
    "binary": "0.3.0",
    "flow-bin": "0.109.0",
    "prettier": "1.18.2",
    "s2s": "0.20.1",
    "serve": "10.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "now-start": "serve build/",
    "s2s": "s2s",
    "deploy": "firebase deploy",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "babel-plugin-styled-components",
      {
        "ssr": false
      }
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
